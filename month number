// write c program to input month niumber and print number of days in that month
#include <stdio.h>

int main() {
   int n;
   printf("Enter the number");
   scanf("%d",&n);
   if(n ==1 || n==3 || n==5 || n==7 || n==9 || n==11)
   {
       printf("The number of days in that month is 31");
   }
   else if(n==4 || n==6 || n==8 || n==10 || n==12)
   {
       printf("The number of days in this month is 30");
   }
   else if(n==2)
   {
       printf("The number of days in this month is 28 or 29");
   }
   else
   {
       printf("You can entered invalid data");
   }

    return 0;
}
